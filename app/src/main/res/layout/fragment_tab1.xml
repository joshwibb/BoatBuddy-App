<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Tab1"
        android:id="@+id/constraintLayoutTab1">

    <LinearLayout
            android:layout_width="0dp"
            android:layout_height="100dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/linearLayout2">

        <ImageView
                android:src="@drawable/ic_black_circle_svgrepo_com"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageOneShort"
                android:background="#00FFFFFF"
                tools:layout_marginStart="100dp"
                android:layout_marginTop="0dp"
                android:layout_gravity="center" />

        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="125dp">

            <TextView
                    android:text="@string/oneshortblast"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/textViewOneShort"
                    android:layout_marginEnd="0dp"
                    android:layout_weight="1"
                    android:gravity="left|center_vertical" />

            <TextView
                    android:text="@string/oneshortblastmeaning"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/textViewOneShortMeaning"
                    android:layout_marginEnd="0dp"
                    android:layout_weight="1"
                    android:gravity="start|left" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
            android:layout_width="0dp"
            android:layout_height="100dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
            android:id="@+id/linearLayout3">

        <ImageView
                android:src="@drawable/ic_black_circle_svgrepo_com"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ImageViewTwoShort1"
                android:background="#00FFFFFF"
                tools:layout_marginStart="60dp"
                android:layout_marginTop="0dp"
                android:layout_gravity="center" />

        <ImageView
                android:src="@drawable/ic_black_circle_svgrepo_com"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ImageViewTwoShort2"
                android:background="#00FFFFFF"
                tools:layout_marginStart="10dp"
                android:layout_marginTop="0dp"
                android:layout_gravity="center" />

        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="95dp">

            <TextView
                    android:text="@string/twoshortblast"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/textViewTwoShort"
                    android:layout_marginEnd="0dp"
                    android:layout_weight="1"
                    android:gravity="left|center_vertical" />

            <TextView
                    android:text="@string/twoshortblastmeaning"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/textViewTwoShortMeaning"
                    android:layout_marginEnd="0dp"
                    android:layout_weight="1"
                    android:gravity="start" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
            android:layout_width="0dp"
            android:layout_height="100dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
            android:id="@+id/linearLayout4">

        <ImageView
                android:src="@drawable/ic_black_circle_svgrepo_com"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ImageViewThreeShort2"
                android:background="#00FFFFFF"
                tools:layout_marginStart="20dp"
                android:layout_marginTop="0dp"
                android:layout_gravity="center" />

        <ImageView
                android:src="@drawable/ic_black_circle_svgrepo_com"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ImageViewThreeShort3"
                android:background="#00FFFFFF"
                tools:layout_marginStart="10dp"
                android:layout_marginTop="0dp"
                android:layout_gravity="center" />

        <ImageView
                android:src="@drawable/ic_black_circle_svgrepo_com"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ImageViewThreeShort1"
                android:background="#00FFFFFF"
                tools:layout_marginStart="10dp"
                android:layout_marginTop="0dp"
                android:layout_gravity="center" />

        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="65dp">

            <TextView
                    android:text="@string/threeshortblast"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/textViewThreeShort"
                    android:layout_marginEnd="0dp"
                    android:layout_weight="1"
                    android:gravity="left|center_vertical" />

            <TextView
                    android:text="@string/threeshortblastmeaning"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/textViewThreeShortMeaning"
                    android:layout_marginEnd="0dp"
                    android:layout_weight="1"
                    android:gravity="start" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
            android:layout_width="0dp"
            android:layout_height="100dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout4"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/linearLayout5">

        <ImageView
                android:src="@drawable/ic_black_circle_svgrepo_com"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ImageViewFiveShort1"
                android:background="#00FFFFFF"
                tools:layout_marginStart="5dp"
                android:layout_marginTop="0dp"
                android:layout_gravity="center" />

        <ImageView
                android:src="@drawable/ic_black_circle_svgrepo_com"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ImageViewFiveShort2"
                android:background="#00FFFFFF"
                tools:layout_marginStart="5dp"
                android:layout_marginTop="0dp"
                android:layout_gravity="center" />

        <ImageView
                android:src="@drawable/ic_black_circle_svgrepo_com"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ImageViewFiveShort3"
                android:background="#00FFFFFF"
                tools:layout_marginStart="5dp"
                android:layout_marginTop="0dp"
                android:layout_gravity="center" />

        <ImageView
                android:src="@drawable/ic_black_circle_svgrepo_com"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ImageViewFiveShort4"
                android:background="#00FFFFFF"
                tools:layout_marginStart="5dp"
                android:layout_marginTop="0dp"
                android:layout_gravity="center" />

        <ImageView
                android:src="@drawable/ic_black_circle_svgrepo_com"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ImageViewFiveShort5"
                android:background="#00FFFFFF"
                tools:layout_marginStart="5dp"
                android:layout_marginTop="0dp"
                android:layout_gravity="center" />

        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="10dp">

            <TextView
                    android:text="@string/fiveshortblast"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/textViewFiveShort"
                    android:layout_marginEnd="0dp"
                    android:layout_weight="1"
                    android:gravity="left|center_vertical" />

            <TextView
                    android:text="@string/fiveshortblastmeaning"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/textViewFiveShortMeaning"
                    android:layout_marginEnd="0dp"
                    android:layout_weight="1"
                    android:gravity="start" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
            android:layout_width="0dp"
            android:layout_height="100dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout5"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/linearLayout6">

        <ImageView
                android:src="@drawable/longblast"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:id="@+id/imageOneLong"
                android:background="#00FFFFFF"
                tools:layout_marginStart="50dp"
                android:layout_marginTop="0dp"
                android:layout_gravity="center" />

        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="55dp">

            <TextView
                    android:text="@string/onelongblast"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/textViewOneLong"
                    android:layout_marginEnd="0dp"
                    android:layout_weight="1"
                    android:gravity="left|center_vertical" />

            <TextView
                    android:text="@string/onelongblastmeaning"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/textViewOneLongMeaning"
                    android:layout_marginEnd="0dp"
                    android:layout_weight="1"
                    android:gravity="start" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout6"
            android:layout_marginTop="8dp">

        <TextView
                android:text="@string/blasttypeshort"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView6"
                android:textSize="24sp"
                android:gravity="center" />

        <TextView
                android:text="@string/blasttypelong"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView7"
                android:textSize="24sp"
                android:gravity="center"
                android:layout_marginTop="10dp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>